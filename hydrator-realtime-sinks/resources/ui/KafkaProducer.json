{
  "id": "KafkaProducer",
  "groups": {
    "position": [ "group1", "group2" ],
    "group1": {
      "display": "Kafka Producer Config",
      "position": [ "brokerlist", "requireacks", "topics" ],
      "fields": {
        "brokerlist": {
          "widget": "csv",
          "label": "Zookeeper Qourum",
          "properties": {
            "delimiter": ","
          }
        },
        "requireacks": {
          "widget": "select",
          "label": "Acknowledge Message",
          "properties": {
            "values" : [ "TRUE", "FALSE" ],
            "default": "FALSE"
          }
        },
        "topics": {
          "widget": "csv",
          "label": "Kafka Topics",
          "properties": {
            "delimiter": ","
          }
        }
      }
    },
    "group2" : {
      "display": "Message Config",
      "position": [ "partitioner", "partitionfield", "key", "format" ],
      "partitioner": {
        "widget": "select",
        "label": "Message Partitioner",
        "properties": {
          "values": [ "Partition Based", "Round Robin"],
          "default" : "Partition Based"
        }
      },
      "fields": {
        "partitionfield": {
          "widget": "textbox",
          "label": "Partition Field (Partition Based)"
        },
        "key": {
          "widget": "textbox",
          "label": "Message Key field"
        },
        "format": {
          "widget": "select",
          "label": "Message Format",
          "properties": {
            "values": [ "JSON", "CSV", "EXCEL", "MYSQL", "TDF", "RFC4180" ],
            "default" : "CSV"
          }
        }
      }
    }
  }
}
