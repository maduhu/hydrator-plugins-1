{
  "id": "KafkaProducer",
  "groups": {
    "position": [ "group1", "group2" ],
    "group1": {
      "display": "Kafka Producer and Topic Config",
      "position": [ "brokers", "topics", "ackrequired" ],
      "fields": {
        "brokers": {
          "widget": "csv",
          "label": "Zookeeper Qourum",
          "properties": {
            "delimiter": ","
          }
        },
        "topics": {
          "widget": "csv",
          "label": "Kafka Topics",
          "properties": {
            "delimiter": ","
          }
        },
        "ackrequired": {
          "widget": "select",
          "label": "Acknowledge Message",
          "properties": {
            "values" : [ "TRUE", "FALSE" ],
            "default": "FALSE"
          }
        }
      }
    },
    "group2" : {
      "display": "Message Configuration",
      "position": [ "format", "partitionfield", "key" ],
      "fields": {
        "format": {
          "widget": "select",
          "label": "Message Format",
          "properties": {
            "values": [ "JSON", "CSV", "EXCEL", "MYSQL", "TDF", "RFC4180" ],
            "default" : "CSV"
          }
        },
        "partitionfield": {
          "widget": "textbox",
          "label": "Partition Field (Partition Based)"
        },
        "key": {
          "widget": "textbox",
          "label": "Message Key field"
        }
      }
    }
  }
}
